{
  "expo": {
    "name": "TravelCostApp",
    "slug": "TravelCostApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iosAppIcon2.jpeg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/launchscreen.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
