{
  "cli": {
    "version": "^3.13.3",
    "appVersionSource": "local",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "staging"
    },
    "development-simulator": {
      "developmentClient": true,
      "channel": "staging",
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "beta1": {
      "distribution": "internal",
      "channel": "staging"
    },
    "preview": {
      "channel": "staging",

      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal"
    },
    "preview2": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      },
      "channel": "staging"
    },
    "preview3": {
      "developmentClient": true,
      "channel": "staging"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "hionoe@hotmail.de",
        "ascAppId": "6446042796",
        "appleTeamId": "AK62F8767Z"
      }
    }
  }
}
