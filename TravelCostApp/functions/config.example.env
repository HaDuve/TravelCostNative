# Signal CLI REST API Configuration
# Copy this file to .env and fill in your values

# Signal API endpoint (local or remote)
SIGNAL_API_URL=http://localhost:8080

# Your phone number (the one registered with Signal CLI)
SIGNAL_SENDER=+1234567890

# Recipient phone number (where notifications will be sent)
SIGNAL_RECIPIENT=+1234567890

# Firebase project configuration
FIREBASE_PROJECT_ID=travelcostnative
FIREBASE_DATABASE_URL=https://travelcostnative-default-rtdb.asia-southeast1.firebasedatabase.app
